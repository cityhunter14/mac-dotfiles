// 将设置放入此文件中以覆盖默认设置
{
    "terminal.external.osxExec": "iTerm.app",
    "editor.renderWhitespace": "all",
    "editor.minimap.enabled": false,
    "vim.hlsearch": true,
    "vim.useSystemClipboard": true,
    "vim.enableNeovim": true,
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.disableAnnoyingNeovimMessage": true,
    "vim.easymotion": true,
    "vim.statusBarColorControl": true,
    "vim.statusBarColors": {
        "normal": "#005f5f",
        "insert": "#5f0000",
        "visual": "#5f00af",
        "visualline": "#005f87",
        "visualblock": "#86592d",
        "replace": "#000000"
    },
    "dash.docset.typescript": [
        "lodash",
        "nodejs",
        "javascript",
        "jquery",
        "jqueryui",
        "jquerym",
        "react",
        "angularjs",
        "backbone",
        "marionette",
        "meteor",
        "sproutcore",
        "moo",
        "prototype",
        "bootstrap",
        "foundation",
        "underscore",
        "ember",
        "sencha",
        "extjs",
        "titanium",
        "knockout",
        "zepto",
        "yui",
        "d3",
        "svg",
        "dojo",
        "coffee",
        "express",
        "grunt",
        "mongoose",
        "moment",
        "require",
        "awsjs",
        "jasmine",
        "sails",
        "sinon",
        "chai",
        "html",
        "css",
        "cordova",
        "phonegap",
        "unity3d",
        "typescript"
    ],
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorCustomizations": {
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f"
    },
    "workbench.activityBar.visible": true,
    "files.associations": {
        "*.tpl": "html"
    }
}

